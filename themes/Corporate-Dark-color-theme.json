    "peekViewResult.background": "#121212",
    "peekViewTitle.background": "#1F1F1F"
  },
  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#64748B",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["variable", "string.quoted.single", "string.quoted.double"],
      "settings": {
        "foreground": "#34D399"
      }
    },
    {
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#818CF8"
      }
    },
    {
      "name": "External Calls and Custom Support",
      "scope": [
        "support.function",
        "support.class",
        "support.type",
        "entity.name.function.external",
        "entity.name.class.external",
        "variable.other.object.external",
        "meta.function-call.external"
      ],
      "settings": {
        "foreground": "#EF7F1A",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["constant.numeric", "constant.language", "support.constant"],
      "settings": {
        "foreground": "#FBBF24"
      }
    },
    {
      "scope": ["entity.name.function", "meta.function-call", "support.function"],
      "settings": {
        "foreground": "#38BDF8"
      }
    },
    {
      "scope": ["entity.name.type", "entity.name.class", "support.class"],
      "settings": {
        "foreground": "#F472B6"
      }
    },
    {
      "scope": ["punctuation", "meta.brace", "storage.type.function.arrow"],
      "settings": {
        "foreground": "#94A3B8"
      }
    }
  ],
  "semanticHighlighting": true
}
