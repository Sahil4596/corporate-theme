  "tokenColors": [
    {
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#64748B",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "variable",
        "string.quoted.single",
        "string.quoted.double"
      ],
      "settings": {
        "foreground": "#2DD4BF"
      }
    },
    {
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#60A5FA",
        "fontStyle": "bold"
      }
    },
    {
      "name": "External Calls and Custom Support",
      "scope": [
        "support.function",
        "support.class",
        "support.type",
        "entity.name.function.external",
        "entity.name.class.external",
        "variable.other.object.external",
        "meta.function-call.external"
      ],
      "settings": {
        "foreground": "#D97706",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language",
        "support.constant"
      ],
      "settings": {
        "foreground": "#FBBF24"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "support.function"
      ],
      "settings": {
        "foreground": "#60A5FA"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "support.class"
      ],
      "settings": {
        "foreground": "#C084FC"
      }
    },
    {
      "scope": [
        "punctuation",
        "meta.brace",
        "storage.type.function.arrow"
      ],
      "settings": {
        "foreground": "#94A3B8"
      }
    }
  ],
  "semanticHighlighting": true
}